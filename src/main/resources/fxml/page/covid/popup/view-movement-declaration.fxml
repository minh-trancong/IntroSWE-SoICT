                                <MFXTextField fx:id="licensePlateTextField" allowEdit="false" floatMode="BORDER" floatingText="Biển số" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" GridPane.rowIndex="4" />
                                <MFXTextField fx:id="historyTextField" allowEdit="false" floatMode="BORDER" floatingText="Lịch sử" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" GridPane.rowIndex="5" />
                        <MFXTextField fx:id="declarationDateTextField" allowEdit="false" floatMode="BORDER" floatingText="Ngày khai báo" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" GridPane.columnSpan="2" GridPane.rowIndex="6" />
