                            </columnConstraints>
                            <rowConstraints>
                                <RowConstraints maxHeight="1.7976931348623157E308" vgrow="NEVER" />
                                <RowConstraints maxHeight="1.7976931348623157E308" vgrow="NEVER" />
                                <RowConstraints maxHeight="1.7976931348623157E308" vgrow="NEVER" />
                                <RowConstraints maxHeight="1.7976931348623157E308" vgrow="NEVER" />
                                <RowConstraints maxHeight="1.7976931348623157E308" vgrow="NEVER" />
                                <RowConstraints maxHeight="1.7976931348623157E308" vgrow="NEVER" />
                        <RowConstraints maxHeight="1.7976931348623157E308" vgrow="NEVER" />




                            </rowConstraints>
                            <children>
                                <MFXTextField fx:id="nameTextField" allowEdit="false" floatMode="BORDER" floatingText="Họ và tên" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minWidth="256.0" />
                                <MFXTextField fx:id="departureTextField" allowEdit="false" floatMode="BORDER" floatingText="Điểm khởi hành" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minWidth="256.0" GridPane.rowIndex="1" />
                                <MFXTextField fx:id="citizenIdTextField" allowEdit="false" floatMode="BORDER" floatingText="Chứng minh nhân dân" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minWidth="256.0" GridPane.columnIndex="1" />
                                <MFXTextField fx:id="destinationTextField" allowEdit="false" floatMode="BORDER" floatingText="Điểm đến" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" GridPane.columnSpan="2" GridPane.rowIndex="2" />
                                <MFXTextField fx:id="passengerNumberTextField" allowEdit="false" floatMode="BORDER" floatingText="Số hành khách" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" GridPane.rowIndex="5" />
