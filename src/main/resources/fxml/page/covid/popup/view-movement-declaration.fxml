                        <MFXTextField fx:id="covidTextField" allowEdit="false" floatMode="BORDER" floatingText="Bị COVID" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" GridPane.columnIndex="1" GridPane.rowIndex="3" />
                            </children>
                     <HBox.margin>
                        <Insets bottom="20.0" />
                     </HBox.margin>
                        </GridPane>
                    </children>
                </HBox>
            </center>
            <padding>
                <Insets bottom="20.0" left="80.0" right="80.0" top="20.0" />
            </padding>
        </BorderPane>
    </content>
</ScrollPane>
